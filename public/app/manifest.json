{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "BoardZone",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.120.0",
    "author": "Toneko",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "launcher_icon": "desktop-icon.ico",
    "window_icon": "TaskbarIcon.png",
    "description": "Lorem Ipsum is simply dummy text of the printing and typesetting industry."
  },
  "permissions": ["GameInfo"],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [21626]
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [21626]
        },
        "start_minimized": true
      }
    ],
    "hotkeys": {
      "sample_app_showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      }
    },
    "game_events": [21626],
    "start_window": "background",
    "windows": {
      "background": {
        "file": "./Files/index.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "./Files/index.html",
        "native_window": true,
        "transparent": true,
        "resizable": true,
        "size": {
          "width": 1150,
          "height": 400
        },
        "min_size": {
          "width": 850,
          "height": 400
        },
        "max_size": {
          "width": 1212,
          "height": 600
        },
        "desktop_only": true
      },
      "in_game": {
        "file": "./Files/index.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "showhide",
        "transparent": true,
        "resizable": true,
        "override_on_update": true,
        "size": {
          "width": 230,
          "height": 128
        },
        "min_size": {
          "width": 230,
          "height": 128
        },
        "max_size": {
          "width": 230,
          "height": 128
        },
        "start_position": {
          "top": 500,
          "left": 30
        }
      }
    },
    "block_top_window_navigation": true,
    "popup_blocker": true,
    "mute": true,
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
